if exists libs:reqtools.library
	echo "Auf Ihrem System ist "
	version libs:reqtools.library
	echo "installiert."
	ask "Soll sie durch reqtools-Version 38.1434 überschrieben werden?"
	if warn
		echo Kopiere reqtools.library
		copy reqtools.library libs:
	endif
else
	echo Kopiere reqtools.library
	copy reqtools.library libs:
endif
