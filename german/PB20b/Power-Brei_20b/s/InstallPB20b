PATH PB20b:c ADD
ECHO "Freeware - Power-Brei #20b (9-Jul-95)*N"
FAILAT 30
PATH RAM: ADD
MAKEDIR >NIL: RAM:T
MAKEDIR >NIL: ram:env
ASSIGN >NIL: ENV: ram:env
ASSIGN >NIL: T: RAM:T
MAKEDIR >NIL: T:PB-Brushes

CD PB20b:
COPY Brushes/LongBar TO T:PB-Brushes/
COPY Brushes/MusikAn TO T:PB-Brushes/
COPY Brushes/MusikAus TO T:PB-Brushes/
COPY Brushes/Juli95  TO T:PB-Brushes/

LookV3.51 Menue BITPLANES 3 CONFIG s/Look.config
StopSMP
EndCLI
