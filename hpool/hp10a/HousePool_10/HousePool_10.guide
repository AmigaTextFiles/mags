@{"INDEX" link "index2" 0}@{"AKTUELL" link "aktuell" 0}@{"MUSIK" link "musik" 0}@{"LIFESTYLE" link "l-style" 0}@{"ARTIKEL" link "artikel" 0}@{"BERICHTE" link "berichte" 0}@{"PRÄSENT." link "present" 0}@{"PROFILE" link "profile" 0}@{"ALLGEMEIN" link "general" 0}@{"SCHLUß" link "schluss" 0}
