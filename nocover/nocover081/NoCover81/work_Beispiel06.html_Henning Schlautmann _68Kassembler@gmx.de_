<html>
<head>
<script language="Javascript">
function Daten(eingabe)
{
 var eingabe;
 var eingabe2="Ihr Name ist:\n";
 eingabe2=eingabe2+eingabe;
 alert(eingabe2);
}
function Name()
{
 var eingabeName;
 eingabeName=prompt("Geben sie Ihren Namen ein.");
 Daten(eingabeName);
}
</script>
</head>
<body onload="Name()">
</body>
</html>
