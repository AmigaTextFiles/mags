<html>
<head>
<script language="Javascript">
function eingabe()
{
 var ein;
 ein=prompt("Bitte geben sie ein Wert ein!");
 return(ein);
}
function vergleich(ein)
{
 var ein;
 if(ein=="Wert" || ein=="wert")
 {
  alert("Sie haben den Richtigen Wert eingegeben");
 }else{
  alert("Sie haben was falsches eingegeben!");
 }
}
function main()
{
 var ein;
 ein=eingabe();
 vergleich(ein);
}
</script>
</head>
<body onload="main()">
</body>
</html>